{
  "version": 3,
  "projects": {},
  "selectedDraftVaultPath": "docs/Projects/impractical Architecture/Overview/Index.md",
  "userScriptFolder": null,
  "sessionStorage": "file",
  "sessions": [],
  "showWordCountInStatusBar": true,
  "startNewSessionEachDay": true,
  "sessionGoal": 1000,
  "applyGoalTo": "project",
  "notifyOnGoal": true,
  "countDeletionsForGoal": true,
  "keepSessionCount": 30,
  "sessionFile": "longform-sessions.json",
  "numberScenes": true,
  "sceneTemplate": "Templates/Note Template.md",
  "workflows": {
    "Default Workflow": {
      "name": "Default Workflow",
      "description": "A starter workflow. Feel free to edit, rename, or delete it and create your own.",
      "steps": [
        {
          "id": "strip-frontmatter",
          "optionValues": {}
        },
        {
          "id": "remove-links",
          "optionValues": {
            "remove-wikilinks": false,
            "remove-external-links": false
          }
        },
        {
          "id": "prepend-title",
          "optionValues": {
            "format": "$3{#} $1",
            "separator": "\n\n"
          }
        },
        {
          "id": "concatenate-text",
          "optionValues": {
            "separator": "\\n\\n---\\n\\n"
          }
        },
        {
          "id": "write-to-note",
          "optionValues": {
            "target": "$1 manuscript.md",
            "open-after": true
          }
        }
      ]
    }
  }
}